{
  "name": "@hll/stone-business",
  "version": "0.0.7",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "father-build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test -w",
    "test:coverage": "umi-test --coverage"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@types/lodash": "^4.14.179",
    "@umijs/preset-react": "^2.1.1",
    "@umijs/test": "^3.0.5",
    "antd": "^4.18.8",
    "axios": "^0.26.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.8.25",
    "js-cookie": "^2.2.1",
    "lint-staged": "^10.0.7",
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.2.1",
    "@types/js-cookie": "^2.2.6",
    "dumi": "^1.1.38",
    "father-build": "^1.21.1",
    "prettier": "^2.5.1",
    "yorkie": "^2.0.0"
  }
}
